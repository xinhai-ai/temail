{
  "name": "temail",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "predev": "prisma generate",
    "dev": "next dev",
    "dev:all": "concurrently -n next,imap -c blue,green \"cross-env IMAP_SERVICE_ENABLED=1 next dev\" \"npm run imap:service\"",
    "prebuild": "prisma generate",
    "build": "next build",
    "start": "next start",
    "start:all": "concurrently -n next,imap -c blue,green \"cross-env IMAP_SERVICE_ENABLED=1 next start\" \"npm run imap:service\"",
    "lint": "eslint",
    "release:patch": "npm version patch -m \"chore(release): v%s\"",
    "release:minor": "npm version minor -m \"chore(release): v%s\"",
    "release:major": "npm version major -m \"chore(release): v%s\"",
    "test": "node --test --import tsx src/**/*.test.ts",
    "imap:test": "dotenv -- node --conditions=react-server --import tsx scripts/imap-tester.ts",
    "imap:service": "dotenv -- node --conditions=react-server --import tsx scripts/imap-service.ts"
  },
